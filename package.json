{
  "name": "vscode-antd-hero",
  "displayName": "Ant-Design hero",
  "description": "Your Ant-Design hero",
  "version": "0.0.1",
  "publisher": "fi3ework",
  "repository": {
    "type": "git",
    "url": "https://github.com/fi3ework/vscode-antd-hero"
  },
  "bugs": {
    "url": "https://github.com/fi3ework/vscode-antd-hero/issues"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "capabilities": {
      "hoverProvider": "true"
    },
    "commands": [
      {
        "command": "antdHero.afterCompletion",
        "title": "[antd hero internal] replace string"
      }
    ],
    "languages": [
      {
        "id": "locations",
        "aliases": [
          "Locations"
        ],
        "extensions": [
          ".locations"
        ]
      }
    ],
    "configuration": {
      "title": "Antd Hero",
      "properties": {
        "antdHero.language": {
          "type": "string",
          "default": "English",
          "enum": [
            "English",
            "中文"
          ],
          "description": "Language of Ant Design documentation."
        },
        "antdHero.handlerPrefix": {
          "type": "string",
          "default": "handle",
          "minLength": 1,
          "description": "Prefix of automatically generated handler."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./",
    "build-doc": "ts-node ./src/buildResource/index.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^13.1.1",
    "@types/vscode": "^1.39.0",
    "ts-node": "^8.5.4"
  },
  "dependencies": {
    "@octokit/rest": "^16.35.2",
    "decamelize": "^3.2.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "remark-parse": "^7.0.2",
    "remark-stringify": "^7.0.4",
    "tiny-invariant": "^1.0.6",
    "typescript": "^3.7.4",
    "unified": "^8.4.2",
    "unist-util-find": "^1.0.1",
    "unist-util-find-after": "^2.0.4",
    "unist-util-visit-all-after": "^1.0.0",
    "vscode-uri": "^2.1.1"
  }
}
