{
  "name": "vscode-antd-rush",
  "displayName": "Antd Rush",
  "description": "Rush to Ant Design",
  "version": "0.0.1",
  "publisher": "fi3ework",
  "repository": {
    "type": "git",
    "url": "https://github.com/fi3ework/vscode-antd-rush"
  },
  "bugs": {
    "url": "https://github.com/fi3ework/vscode-antd-rush/issues"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon.png",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "capabilities": {
      "hoverProvider": "true"
    },
    "commands": [
      {
        "command": "antdRush.afterCompletion",
        "title": "[antd rush internal] replace string"
      }
    ],
    "languages": [
      {
        "id": "locations",
        "aliases": [
          "Locations"
        ],
        "extensions": [
          ".locations"
        ]
      }
    ],
    "configuration": {
      "title": "Antd Rush",
      "properties": {
        "antdRush.language": {
          "type": "string",
          "default": "中文",
          "enum": [
            "English",
            "中文"
          ],
          "description": "Language of Ant Design documentation."
        },
        "antdRush.handlerPrefix": {
          "type": "string",
          "default": "handle",
          "minLength": 1,
          "description": "Prefix of automatically generated handler."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./src",
    "compile-test": "tsc -p ./test",
    "lint": "tslint -p ./src",
    "watch": "tsc -watch -p ./src",
    "watch-test": "tsc -watch -p ./test",
    "build-doc": "ts-node ./src/buildResource/index.ts",
    "pretest": "npm run compile-test",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/js-base64": "^2.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.6",
    "ts-node": "^8.5.4"
  },
  "dependencies": {
    "@octokit/rest": "^16.35.2",
    "antd": "^3.26.7",
    "decamelize": "^3.2.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "mocha": "^7.0.0",
    "remark-parse": "^7.0.2",
    "remark-stringify": "^7.0.4",
    "typescript": "^3.7.4",
    "unified": "^8.4.2",
    "unist-util-find": "^1.0.1",
    "vscode-languageserver": "^6.1.0",
    "vscode-test": "^1.3.0",
    "vscode-uri": "^2.1.1"
  }
}
